{
  "name": "@SophiaTX/alexandria-js",
  "version": "0.0.1",
  "description": "Alexandria.js the JavaScript API for Sophia blockchain",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run build-browser && npm run build-node",
    "build-browser": "rm -rf dist && NODE_ENV=production node ./node_modules/webpack/bin/webpack.js && gzip -k -f ./dist/*.js && du -h ./dist/*",
    "build-node": "mkdir -p ./lib && cp -r ./src/* ./lib/ && babel ./src --out-dir ./lib",
    "prepublish": "npm run build"
  },
  "browser": {
    "ws": false,
    "crypto": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sophiatx/alexandria.js.git"
  },
  "keywords": [
    "sophia",
    "sophiatx",
    "blockchain",
    "alexandriajs"
  ],
  "babel": {
    "presets": [
      "stage-3",
      "latest"
    ]
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "semi": 2
    }
  },
  "author": "Fabien (https://github.com/bonustrack)",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.18.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "bigi": "^1.4.2",
    "bluebird": "^3.4.6",
    "browserify-aes": "^1.0.6",
    "bs58": "^4.0.0",
    "buffer": "^5.0.6",
    "bytebuffer": "^5.0.1",
    "create-hash": "^1.1.2",
    "create-hmac": "^1.1.4",
    "cross-env": "^5.0.0",
    "cross-fetch": "^1.1.1",
    "debug": "^2.6.8",
    "detect-node": "^2.0.3",
    "ecurve": "^1.0.5",
    "lodash": "^4.16.4",
    "mocha": "3.2.0",
    "pug": "^2.0.3",
    "randombytes": "^2.0.6",
    "secure-random": "^1.1.1",
    "ws": "^3.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-register": "^6.14.0",
    "eslint": "^3.5.0",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.2.1",
    "json-loader": "^0.5.4",
    "mocha": "3.2.0",
    "mocha-make-stub": "^2.3.2",
    "should": "^11.1.0",
    "webpack": "^1.13.2",
    "webpack-visualizer-plugin": "^0.1.5"
  },
  "contributors": []
}
